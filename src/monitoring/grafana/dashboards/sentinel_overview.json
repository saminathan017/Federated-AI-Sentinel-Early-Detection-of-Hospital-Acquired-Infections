{
  "dashboard": {
    "title": "Federated AI Sentinel Overview",
    "tags": ["infection-prediction", "federated-learning"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"infection-risk-api\"}[5m])",
            "legendFormat": "{{method}} {{handler}}"
          }
        ],
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
      },
      {
        "id": 2,
        "title": "API Latency (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"infection-risk-api\"}[5m]))",
            "legendFormat": "p95 latency"
          }
        ],
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8}
      },
      {
        "id": 3,
        "title": "Predictions by Risk Level",
        "type": "pie",
        "targets": [
          {
            "expr": "infection_predictions_total",
            "legendFormat": "{{outcome}}"
          }
        ],
        "gridPos": {"x": 0, "y": 8, "w": 12, "h": 8}
      },
      {
        "id": 4,
        "title": "Prediction Latency Distribution",
        "type": "heatmap",
        "targets": [
          {
            "expr": "rate(infection_prediction_latency_seconds_bucket[5m])",
            "legendFormat": "{{le}}"
          }
        ],
        "gridPos": {"x": 12, "y": 8, "w": 12, "h": 8}
      },
      {
        "id": 5,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"infection-risk-api\",status=~\"5..\"}[5m])",
            "legendFormat": "5xx errors"
          }
        ],
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8}
      },
      {
        "id": 6,
        "title": "Active API Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "http_requests_inprogress{job=\"infection-risk-api\"}"
          }
        ],
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8}
      }
    ],
    "refresh": "10s",
    "schemaVersion": 27,
    "version": 1
  }
}

